	"population": {
		"citizens": {
			"template": {
				"@gwsclass": "Entity",
				"@gwsgroups": ["Citizen"],
				"color": "Gray",
				"wait_for_me": true,
				"@behaviours": [{
					"@gwsclass": "CalculateGTAlgRouteBehaviour",
					"start": true,
					"id": "GTALG",
					"transport_mode": "<vehicle_type>",
					"route_optimization": "QUICKER",
					"destination_json": "<destination>",
					"stop_if_no_route": true,
					"duration": 1,
					"nexts": ["NEXT_DESTINATION"]
				}, {
					"@gwsclass": "SetNextRouteDestinationBehaviour",
					"id": "NEXT_DESTINATION",
					"duration": 1,
					"nexts_while_pending_route": ["REMOVE_WAIT"],
					"nexts_if_finished": ["STOP"]
				}, {
					"@gwsclass": "SetPropertyBehaviour",
					"id": "REMOVE_WAIT",
					"entity_to_set_id": "<id>",
					"properties": {
						"wait_for_me": false
					},
					"duration": 1,
					"nexts": ["DISPLAY"]
				}, {
					"@gwsclass": "SendEntitySnapshotBehaviour",
					"id": "DISPLAY",
					"entity_to_send_id": "<id>",
					"duration": 1,
					"nexts": ["MOVE"]
				}, {
					"@gwsclass": "MoveThroughNetworkBehaviour",
					"id": "MOVE",
					"duration": 30,
					"input_transport_network_type": "<vehicle_type>",
					"input_route_destination": "<next_destination>",
					"nexts_if_arrived": ["NEXT_DESTINATION"],
					"nexts_if_not_arrived": ["NOISE"]
				}, {
					"@gwsclass": "VehicleNoiseBehaviour",
					"id": "NOISE",
					"input_vehicle_type": "<vehicle_type>",
					"duration": 0,
					"nexts": ["INCREMENT_NOISE_IN_ROAD"]
				}, {    
                                        "@gwsclass": "IncrementPropertyBehaviour", 
                                        "duration": 1, 
                                        "@id": "INCREMENT_NOISE_IN_ROAD", 
                                        "property" : "accumulated_noise" , 
                                        "increment" : "<instant_noise>" , 
                                        "entity_to_increment_id" : "<current_road>" , 
                                        "nexts_if_max" : ["POLLUTE"] , 
                                        "nexts_if_min" : ["POLLUTE"] , 
                                        "nexts_if_within_threshold" : ["POLLUTE"] 
                                },{
					"@gwsclass": "PolluteBehaviour",
					"id": "POLLUTE",
					"input_vehicle_type": "<vehicle_type>",
					"input_vehicle_subtype": "<vehicle_subtype>",
					"input_current_road_gradient": 0,
					"input_current_road_traffic_situation": 0.36,
					"input_pollutant_types": ["DIRECT", "INDIRECT", "FCL", "INDIRECT_ELECTRIC_ES", "INDIRECT_ELECTRIC_PL", "INDIRECT_ELECTRIC_HU", "INDIRECT_ELECTRIC_NL", "WH"],
					"duration": 0,
					"nexts": ["DISPLAY"]
				}, {
					"@gwsclass": "StopEntityBehaviour",
					"id": "STOP",
					"duration": 1
				}]
			},
			"datasources": [{
				"scenario_id": "855440ed-eb06-474c-95fa-735b690086f0-fa4a7d2b-865a-53d2-b510",
				"entity_type": "COACH"
			}, {
				"scenario_id": "855440ed-eb06-474c-95fa-735b690086f0-fa4a7d2b-865a-53d2-b510",
				"entity_type": "HGV"
			}, {

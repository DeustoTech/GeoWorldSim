					"destination_json": "<destination>",
					"stop_if_no_route": true,
					"duration": 1,
					"nexts": ["NEXT_DESTINATION"]
				}, {
					"@gwsclass": "SetNextRouteDestinationBehaviour",
					"id": "NEXT_DESTINATION",
					"duration": 1,
					"nexts_while_pending_route": ["REMOVE_WAIT"],
					"nexts_if_finished": ["STOP"]
				}, {
					"@gwsclass": "SetPropertyBehaviour",
					"id": "REMOVE_WAIT",
					"entity_id": "<id>",
					"properties": {
						"wait_for_me": false
					},
					"duration": 1,
					"nexts": ["DISPLAY"]
				}, {
					"@gwsclass": "SendEntitySnapshotBehaviour",
					"id": "DISPLAY",
					"entity_to_send_id": "<id>",
					"duration": 1,
					"nexts": ["MOVE"]
				}, {
					"@gwsclass": "MoveThroughNetworkBehaviour",
					"id": "MOVE",
					"duration": 30,
					"input_transport_network_type": "<vehicle_type>",
					"input_route_destination": "<next_destination>",
					"nexts_if_arrived": ["NEXT_DESTINATION"],
					"nexts_if_not_arrived": ["NOISE"]
				}, {
					"@gwsclass": "VehicleNoiseBehaviour",
					"id": "NOISE",
					"input_vehicle_type": "<vehicle_type>",
					"duration": 0,
					"nexts": ["POLLUTE"]
				}, {
					"@gwsclass": "PolluteBehaviour",
					"id": "POLLUTE",
					"input_vehicle_type": "<vehicle_type>",
					"input_vehicle_subtype": "<vehicle_subtype>",
					"input_current_road_gradient": 0,
					"input_current_road_traffic_situation": 0.36,
					"input_pollutant_types": ["CO2e", "FC_MJ", "FC"],
					"duration": 0,
					"nexts": ["DISPLAY"]
				}, {
					"@gwsclass": "StopEntityBehaviour",
					"id": "STOP",
					"duration": 1
				}]
			},
